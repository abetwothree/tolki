{
    "name": "@laravel-js/monorepo",
    "version": "0.0.0",
    "description": "",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "run-s build:*",
        "build:arr": "pnpm -F @laravel-js/arr build",
        "build:collection": "pnpm -F @laravel-js/collection build",
        "build:num": "pnpm -F @laravel-js/num build",
        "build:str": "pnpm -F @laravel-js/str build",
        "format": "npx prettier --write . --cache",
        "format:check": "npx prettier --check .",
        "lint": "run-s lint:*",
        "lint:oxlint": "npx oxlint --fix -D correctness --ignore-path .gitignore",
        "lint:eslint": "npx eslint . --fix",
        "lint-migrate": "npx @oxlint/migrate",
        "test": "vitest --run --coverage",
        "test:dev": "vitest --coverage",
        "test:ui": "vitest --ui --coverage",
        "ts:check": "tsc --incremental --noEmit",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs",
        "list:functions": "ts-node ./scripts/functions-list.ts",
        "stubs": "run-s stubs:*",
        "stubs:copy-framework": "ts-node ./scripts/copy-framework-files.ts",
        "stubs:create-lists": "ts-node ./scripts/stubs-function-list-create.ts"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "packageManager": "pnpm@10.12.1",
    "devDependencies": {
        "@eslint/css": "^0.11.1",
        "@eslint/js": "^9.38.0",
        "@eslint/json": "^0.13.2",
        "@eslint/markdown": "^7.4.0",
        "@laravel-js/all": "workspace:^",
        "@laravel-js/arr": "workspace:^",
        "@laravel-js/collection": "workspace:^",
        "@laravel-js/data": "workspace:^",
        "@laravel-js/num": "workspace:^",
        "@laravel-js/obj": "workspace:^",
        "@laravel-js/path": "workspace:^",
        "@laravel-js/str": "workspace:^",
        "@laravel-js/types": "workspace:^",
        "@laravel-js/utils": "workspace:^",
        "@types/node": "^24.8.1",
        "@vitest/coverage-istanbul": "3.2.4",
        "@vitest/ui": "3.2.4",
        "dotenv": "^17.2.3",
        "eslint": "^9.38.0",
        "eslint-plugin-oxlint": "^1.23.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "glob": "^11.0.3",
        "globals": "^16.4.0",
        "jiti": "^2.6.1",
        "jsdom": "^26.1.0",
        "npm-run-all2": "^8.0.4",
        "path": "^0.12.7",
        "prettier": "3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.1",
        "vite": "^7.1.10",
        "vite-plugin-dts": "^4.5.4",
        "vitepress": "2.0.0-alpha.12",
        "vitest": "^3.2.4"
    }
}
