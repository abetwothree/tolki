{
    "name": "@aid/monorepo",
    "version": "0.0.0",
    "description": "",
    "type": "module",
    "private": true,
    "scripts": {
        "ci:publish": "pnpm publish -r --access=public",
        "ci:publish-arr": "pnpm publish --filter @aid/arr --access=public",
        "ci:publish-collection": "pnpm publish --filter @aid/collection --access=public",
        "ci:publish-data": "pnpm publish --filter @aid/data --access=public",
        "ci:publish-num": "pnpm publish --filter @aid/num --access=public",
        "ci:publish-obj": "pnpm publish --filter @aid/obj --access=public",
        "ci:publish-path": "pnpm publish --filter @aid/path --access=public",
        "ci:publish-str": "pnpm publish --filter @aid/str --access=public",
        "ci:publish-utils": "pnpm publish --filter @aid/utils --access=public",
        "build": "run-s build:*",
        "build:arr": "pnpm -F @aid/arr build",
        "build:collection": "pnpm -F @aid/collection build",
        "build:data": "pnpm -F @aid/data build",
        "build:num": "pnpm -F @aid/num build",
        "build:obj": "pnpm -F @aid/obj build",
        "build:path": "pnpm -F @aid/path build",
        "build:str": "pnpm -F @aid/str build",
        "build:utils": "pnpm -F @aid/utils build",
        "document:add": "pnpx @changesets/cli add",
        "document:version": "pnpx @changesets/cli version",
        "document:publish": "pnpx @changesets/cli publish",
        "format": "npx prettier --write . --cache",
        "format:check": "npx prettier --check .",
        "lint": "run-s lint:*",
        "lint:migrate": "rm -f .oxlintrc.json && npx @oxlint/migrate && prettier --write .oxlintrc.json",
        "lint:oxlint": "npx oxlint --fix -D correctness --ignore-path .gitignore",
        "lint:eslint": "npx eslint . --fix",
        "test": "vitest --run --coverage",
        "test:dev": "vitest --coverage",
        "test:ui": "vitest --ui --coverage",
        "ts:check": "tsc --incremental --noEmit",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs",
        "list:functions": "ts-node ./scripts/functions-list.ts",
        "stubs": "run-s stubs:*",
        "stubs:copy-framework": "ts-node ./scripts/copy-framework-files.ts",
        "stubs:create-lists": "ts-node ./scripts/stubs-function-list-create.ts"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "packageManager": "pnpm@10.12.1",
    "devDependencies": {
        "@changesets/cli": "^2.29.8",
        "@eslint/css": "^0.14.1",
        "@eslint/js": "^9.39.2",
        "@eslint/json": "^0.14.0",
        "@eslint/markdown": "^7.5.1",
        "@aid/all": "workspace:^",
        "@aid/arr": "workspace:^",
        "@aid/collection": "workspace:^",
        "@aid/data": "workspace:^",
        "@aid/num": "workspace:^",
        "@aid/obj": "workspace:^",
        "@aid/path": "workspace:^",
        "@aid/str": "workspace:^",
        "@aid/types": "workspace:^",
        "@aid/utils": "workspace:^",
        "@oxlint/migrate": "^1.36.0",
        "@types/node": "^25.0.3",
        "@vitest/coverage-istanbul": "^4.0.16",
        "@vitest/ui": "^4.0.16",
        "dotenv": "^17.2.3",
        "eslint": "^9.39.2",
        "eslint-plugin-oxlint": "^1.36.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "glob": "^13.0.0",
        "globals": "^16.5.0",
        "jiti": "^2.6.1",
        "jsdom": "^27.4.0",
        "npm-run-all2": "^8.0.4",
        "oxlint": "^1.36.0",
        "path": "^0.12.7",
        "prettier": "3.7.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.51.0",
        "vite": "^7.3.0",
        "vite-plugin-dts": "^4.5.4",
        "vitepress": "2.0.0-alpha.12",
        "vitest": "^4.0.16"
    }
}
