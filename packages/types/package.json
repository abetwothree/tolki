{
    "name": "@tolki/types",
    "version": "1.0.1",
    "description": "Utility TypeScript types for @tolki packages and Laravel HTTP responses.",
    "author": "Abraham Arango <https://github.com/abetwothree>",
    "license": "MIT",
    "homepage": "https://tolki.abe.dev/typescript/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/abetwothree/tolki.git",
        "directory": "packages/types"
    },
    "bugs": {
        "url": "https://github.com/abetwothree/tolki/issues"
    },
    "type": "module",
    "types": "src/index.d.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "typescript",
        "types",
        "laravel",
        "models",
        "pagination"
    ],
    "sideEffects": false,
    "packageManager": "pnpm@10.12.1",
    "files": [
        "src"
    ],
    "exports": {
        ".": {
            "types": "./src/index.d.ts"
        },
        "./arrays": {
            "types": "./src/arrays.d.ts"
        },
        "./collections": {
            "types": "./src/collections.d.ts"
        },
        "./data": {
            "types": "./src/data.d.ts"
        },
        "./models": {
            "types": "./src/models.d.ts"
        },
        "./objects": {
            "types": "./src/objects.d.ts"
        },
        "./pagination": {
            "types": "./src/pagination.d.ts"
        },
        "./path-resolve": {
            "types": "./src/path-resolve.d.ts"
        },
        "./responses": {
            "types": "./src/responses.d.ts"
        },
        "./strings": {
            "types": "./src/strings.d.ts"
        },
        "./utils": {
            "types": "./src/utils.d.ts"
        }
    }
}
