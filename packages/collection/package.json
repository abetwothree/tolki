{
    "name": "@tolki/collection",
    "version": "0.0.0",
    "description": "A collection utility similar to Laravel's Collection class.",
    "author": "Abraham Arango <https://github.com/abetwothree>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/abetwothree/tolki.git",
        "directory": "packages/collection"
    },
    "bugs": {
        "url": "https://github.com/abetwothree/tolki/issues"
    },
    "private": true,
    "type": "module",
    "main": "src/index.ts",
    "scripts": {
        "build": "vite build",
        "prepack": "pnpm run build"
    },
    "keywords": [
        "collection",
        "collections",
        "laravel",
        "utils",
        "utilities"
    ],
    "sideEffects": false,
    "packageManager": "pnpm@10.12.1",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "main": "dist/index.js",
        "types": "dist/index.d.ts",
        "exports": {
            ".": {
                "import": "./dist/index.js",
                "require": "./dist/index.js",
                "types": "./dist/index.d.ts"
            },
            "./collection": {
                "import": "./dist/collection.js",
                "types": "./dist/collection.d.ts"
            },
            "./proxy": {
                "import": "./dist/proxy.js",
                "types": "./dist/proxy.d.ts"
            }
        }
    }
}