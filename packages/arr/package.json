{
    "name": "@tolki/arr",
    "version": "0.0.0",
    "description": "A utility for working with arrays.",
    "private": true,
    "type": "module",
    "main": "src/index.ts",
    "scripts": {
        "build": "vite build",
        "prepack": "pnpm run build"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "sideEffects": false,
    "packageManager": "pnpm@10.12.1",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "main": "dist/index.js",
        "types": "dist/index.d.ts",
        "exports": {
            ".": {
