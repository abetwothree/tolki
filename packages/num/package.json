{
    "name": "@zinaid/num",
    "version": "0.0.1",
    "description": "Utilities for working with numbers similar to Laravel's Num class facade.",
    "type": "module",
    "main": "src/index.ts",
    "scripts": {
        "build": "vite build"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "packageManager": "pnpm@10.12.1",
    "dependencies": {
        "to-words": "^4.7.0"
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "main": "dist/index.js",
        "types": "dist/index.d.ts",
        "exports": {
            ".": {
                "import": "./dist/index.js",
                "require": "./dist/index.js",
                "types": "./dist/index.d.ts"
            },
            "./*": {
                "import": "./dist/*.js",
                "require": "./dist/*.js",
                "types": "./dist/*.d.ts"
            }
        }
    }
}
