{
    "name": "@tolki/path",
    "version": "0.0.0",
    "description": "Utilities for working with path-based operations across arrays/objects.",
    "author": "Abraham Arango <https://github.com/abetwothree>",
    "license": "MIT",
    "private": true,
    "type": "module",
    "main": "src/index.ts",
    "scripts": {
        "build": "vite build",
        "prepack": "pnpm run build"
    },
    "keywords": [],
    "sideEffects": false,
    "packageManager": "pnpm@10.12.1",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "main": "dist/index.js",
        "types": "dist/index.d.ts",
        "exports": {
            ".": {
                "import": "./dist/index.js",
                "require": "./dist/index.js",
                "types": "./dist/index.d.ts"
            },
            "./path": {
                "import": "./dist/path.js",
                "types": "./dist/path.d.ts"
            }
        }
    }
}
